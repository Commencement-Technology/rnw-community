{
  "name": "@rnw-community/nestjs-typed-config",
  "version": "0.42.1",
  "private": false,
  "description": "NestJS typed config",
  "keywords": [
    "nestjs",
    "nestjs config",
    "nestjs ts config",
    "nestjs typescript config"
  ],
  "repository": "https://github.com/rnw-community/rnw-community/tree/master/packages/nestjs-typed-config",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "sideEffects": "false",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "ts": "tsc --pretty -p tsconfig.json",
    "prebuild": "yarn clear",
    "build:esm": "tsc --pretty -p tsconfig.build-cjs.json",
    "build:cjs": "tsc --pretty -p tsconfig.build-esm.json",
    "build": "yarn build:esm && yarn build:cjs",
    "postbuild": "rm -rf ./dist/*.tsbuildinfo",
    "lint": "yarn eslint src",
    "lint:fix": "yarn lint --fix src",
    "test": "yarn jest",
    "test:coverage": "yarn jest --coverage",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "clear": "rm -rf coverage && rm -rf dist && rm -f *.tsbuildinfo",
    "clear:deps": "rm -rf ./node_modules && rm -rf ./dist"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "gitHead": "b5608910319390f9773a9d42c3cc828e8e8a1d95",
  "devDependencies": {
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^1.1.7",
    "@rnw-community/shared": "^0.41.0",
    "@types/hapi__joi": "^17.1.7",
    "joi": "^17.1.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.6",
    "@nestjs/config": "^1.1.7",
    "@rnw-community/shared": "^0.41.0",
    "joi": "^17.1.1"
  }
}
