{
  "name": "@rnw-community/nestjs-webpack-swc",
  "version": "0.43.1",
  "private": false,
  "description": "NestJS typed config",
  "keywords": [
    "nestjs",
    "nestjs webpack",
    "nestjs swc",
    "nestjs swc-loader"
  ],
  "repository": "https://github.com/rnw-community/rnw-community/tree/master/packages/nestjs-typed-config",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "sideEffects": "false",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "ts": "tsc --pretty -p tsconfig.json",
    "build:esm": "tsc --pretty -p tsconfig.build-cjs.json",
    "build:cjs": "tsc --pretty -p tsconfig.build-esm.json",
    "build": "yarn build:esm && yarn build:cjs",
    "postbuild": "rm -rf ./dist/*/*.tsbuildinfo",
    "lint": "yarn eslint src",
    "lint:fix": "yarn lint --fix src",
    "test": "yarn jest",
    "test:coverage": "yarn jest --coverage",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "clear": "rm -rf coverage && rm -rf dist && rm -f *.tsbuildinfo",
    "clear:deps": "rm -rf ./node_modules && rm -rf ./dist"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "gitHead": "b5608910319390f9773a9d42c3cc828e8e8a1d95",
  "dependencies": {
    "@rnw-community/shared": "^0.43.1"
  },
  "devDependencies": {
    "@nestjs/common": "^8.0.6",
    "@types/webpack-env": "^1.18.0",
    "@types/webpack-node-externals": "^2.5.3",
    "run-script-webpack-plugin": "^0.1.1",
    "terser-webpack-plugin": "^5.3.5",
    "webpack": "^5.74.0",
    "webpack-node-externals": "^3.0.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.6",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.239",
    "@swc/helpers": "^0.4.7",
    "run-script-webpack-plugin": "^0.1.1",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.5",
    "webpack": "^5.74.0",
    "webpack-node-externals": "^3.0.0"
  }
}
