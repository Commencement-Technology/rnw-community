{
  "name": "@rnw-community/hoverable",
  "version": "0.0.16",
  "private": false,
  "description": "Hover support for React native elements in React native web",
  "repository": "https://github.com/rnw-community/rnw-community/tree/master/packages/hoverable",
  "license": "MIT",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src/**/*"
  ],
  "keywords": [
    "hover",
    "hoverable",
    "react native hover",
    "react native web hover"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc -b --pretty",
    "lint": "tslint --format stylish --project tsconfig.json",
    "lint:fix": "yarn lint --fix",
    "test": "yarn jest",
    "test:coverage": "yarn jest --coverage",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "clear": "rm -rf allure-* _results_",
    "clear:deps": "rm -rf ./node_modules && rm -rf ./dist"
  },
  "dependencies": {
    "@rnw-community/shared": "^0.0.16"
  },
  "devDependencies": {
    "@types/react": "^16.9.34",
    "@types/react-native": "^0.62.4",
    "react": "^16.13.0",
    "react-native": "^0.60"
  },
  "peerDependencies": {
    "react": "^16.13.0",
    "react-native": "^0.60"
  },
  "gitHead": "b5608910319390f9773a9d42c3cc828e8e8a1d95"
}
