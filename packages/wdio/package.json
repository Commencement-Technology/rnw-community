{
  "name": "@rnw-community/wdio",
  "version": "0.38.2",
  "private": false,
  "description": "WDIO commands, page objects and utils",
  "keywords": [
    "wdio",
    "wdio commands",
    "wdio selectors",
    "wdio pageobject"
  ],
  "repository": "https://github.com/rnw-community/rnw-community/tree/master/packages/wdio",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "sideEffects": "false",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "ts": "tsc --pretty -p tsconfig.json",
    "prebuild": "yarn clear",
    "build:esm": "tsc --pretty -p tsconfig.build-cjs.json",
    "build:cjs": "tsc --pretty -p tsconfig.build-esm.json",
    "build": "yarn build:esm && yarn build:cjs",
    "lint": "yarn eslint src",
    "lint:fix": "yarn lint --fix src",
    "test": "yarn jest",
    "test:coverage": "yarn jest --coverage",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "clear": "rm -rf coverage && rm -rf dist && rm -f *.tsbuildinfo",
    "clear:deps": "rm -rf ./node_modules && rm -rf ./dist"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "gitHead": "ed2cec449e634a16dc3a80b4a8cd2d1d28c9f9c4",
  "devDependencies": {
    "@rnw-community/shared": "^0.38.2",
    "@wdio/types": "^7.20.7",
    "expect-type": "^0.13.0",
    "webdriverio": "^7.20.7"
  },
  "peerDependencies": {
    "@rnw-community/shared": "^0.37.0",
    "webdriverio": "^7.20.7"
  }
}
