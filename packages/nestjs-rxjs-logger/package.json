{
  "name": "@rnw-community/nestjs-rxjs-logger",
  "version": "0.5.0",
  "private": false,
  "description": "NestJS RxJS logger",
  "keywords": [
    "nestjs",
    "nestjs logger",
    "rxjs logger"
  ],
  "repository": "https://github.com/rnw-community/rnw-community/tree/master/packages/shared",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "sideEffects": "false",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "ts": "tsc --pretty -p tsconfig.json",
    "build": "tsc --pretty -p tsconfig.build.json",
    "lint": "yarn eslint src",
    "lint:fix": "yarn lint --fix src",
    "test": "yarn jest",
    "test:coverage": "yarn jest --coverage",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "clear": "rm -rf coverage && rm -rf dist",
    "clear:deps": "rm -rf ./node_modules && rm -rf ./dist"
  },
  "gitHead": "b5608910319390f9773a9d42c3cc828e8e8a1d95",
  "dependencies": {
    "rxjs": "^7.3.0"
  },
  "devDependencies": {
    "@nestjs/common": "^8.0.6"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.6"
  }
}
