{
  "name": "@rnw-community/fast-style",
  "version": "0.36.1",
  "private": false,
  "description": "Fast react native flex and font styling",
  "keywords": [
    "object field tree",
    "enum tree",
    "nested enums",
    "typescript enum tree"
  ],
  "repository": "https://github.com/rnw-community/rnw-community/tree/master/packages/fast-style",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "sideEffects": "false",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "ts": "tsc --pretty -p tsconfig.json",
    "build": "tsc --pretty -p tsconfig.build.json",
    "postbuild": "rm -rf ./dist/*.tsbuildinfo",
    "lint": "yarn eslint src",
    "lint:fix": "yarn lint --fix src",
    "test": "yarn jest",
    "test:coverage": "yarn jest --coverage",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "clear": "rm -rf coverage && rm -rf dist && rm -f *.tsbuildinfo",
    "clear:deps": "rm -rf ./node_modules && rm -rf ./dist"
  },
  "peerDependencies": {
    "@rnw-community/object-field-tree": "^0.36.1",
    "@rnw-community/shared": "^0.36.1"
  },
  "devDependencies": {
    "@types/react-native": "^0.64.12",
    "react-native": "^0.64.1"
  },
  "gitHead": "b5608910319390f9773a9d42c3cc828e8e8a1d95"
}
