{
  "name": "@rnw-community/nestjs-rxjs-redis",
  "version": "0.7.4",
  "private": false,
  "description": "NestJS RxJS redis",
  "keywords": [
    "nestjs",
    "nestjs redis",
    "rxjs redis"
  ],
  "repository": "https://github.com/rnw-community/rnw-community/tree/master/packages/shared",
