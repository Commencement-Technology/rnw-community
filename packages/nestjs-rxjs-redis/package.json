    "ioredis": "^4.28.0",
    "nestjs-redis": "^1.3.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^8.0.6",
    "@rnw-community/shared": "^0.6.0",
    "ioredis": "^4.28.0",
    "nestjs-redis": "^1.3.3"
