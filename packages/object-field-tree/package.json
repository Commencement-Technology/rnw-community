{
  "name": "@rnw-community/object-field-tree",
  "version": "0.0.4",
  "private": false,
  "description": "Create a object fields combinations tree with data generator function",
  "repository": "https://github.com/rnw-community/rnw-community/tree/master/packages/object-field-tree",
  "license": "MIT",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src/**/*"
  ],
  "keywords": [
    "object field tree",
    "enum tree",
    "nested enums",
    "typescript enum tree"
  ],
  "scripts": {
    "ts": "tsc -b --pretty",
    "lint": "tslint --format stylish --project tsconfig.json",
    "lint:fix": "yarn lint --fix",
    "test": "yarn jest",
    "test:coverage": "yarn jest --coverage",
    "format": "prettier --write \"./src/**/*.{ts,tsx,js}\"",
    "clear": "rm -rf allure-* _results_",
    "clear:deps": "rm -rf ./node_modules && rm -rf ./dist",
    "ci": "yarn ts && yarn lint && yarn test",
    "build": "tsc",
    "prepare": "yarn build",
    "prepublish": "yarn ts && yarn lint && yarn format && yarn test",
    "preversion": "yarn ci",
    "version": "yarn format && git add -A src",
    "postversion": "git push && git push --tags"
  }
}
