{
  "name": "@rnw-community/rxjs-errors",
  "version": "0.40.0",
  "private": false,
  "description": "RxJS errors utils",
  "keywords": [
    "rxjs error operator",
    "rxjs error handling"
  ],
  "repository": "https://github.com/rnw-community/rnw-community/tree/master/packages/rxjs-errors",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "sideEffects": "false",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "ts": "tsc --pretty -p tsconfig.json",
    "prebuild": "yarn clear",
    "build:esm": "tsc --pretty -p tsconfig.build-cjs.json",
    "build:cjs": "tsc --pretty -p tsconfig.build-esm.json",
    "build": "yarn build:esm && yarn build:cjs",
    "postbuild": "rm -rf ./dist/*.tsbuildinfo",
    "lint": "yarn eslint src",
    "lint:fix": "yarn lint --fix src",
    "test": "yarn jest",
    "test:coverage": "yarn jest --coverage",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "clear": "rm -rf coverage && rm -rf dist && rm -f *.tsbuildinfo",
    "clear:deps": "rm -rf ./node_modules && rm -rf ./dist"
  },
  "gitHead": "b5608910319390f9773a9d42c3cc828e8e8a1d95",
  "peerDependencies": {
    "@rnw-community/shared": "^0.38.1",
    "rxjs": "^7.4.0"
  },
  "devDependencies": {
    "@rnw-community/shared": "^0.38.1",
    "expect-type": "^0.13.0",
    "rxjs": "^7.4.0"
  }
}
