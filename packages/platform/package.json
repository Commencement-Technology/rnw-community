{
  "name": "@rnw-community/platform",
  "version": "0.42.0",
  "private": false,
  "description": "React native web platform utils and helpers",
  "keywords": [
    "platform utils",
    "platform helpers",
    "react",
    "react-native",
    "react native",
    "react-native-web"
  ],
  "repository": "https://github.com/rnw-community/rnw-community/tree/master/packages/platform",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js"
    }
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "sideEffects": "false",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@rnw-community/wdio": "^0.42.0"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": "^1.13.4",
    "react": "^17.0.2",
    "react-native": "^0.64.1",
    "react-native-config": "^1.4.2",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@react-native-async-storage/async-storage": "^1.13.4",
    "react-native-config": "^1.4.2",
    "redux-persist": "^6.0.0"
  },
  "scripts": {
    "ts": "tsc --pretty -p tsconfig.json",
    "prebuild": "yarn clear",
    "build:esm": "tsc --pretty -p tsconfig.build-cjs.json",
    "build:cjs": "tsc --pretty -p tsconfig.build-esm.json",
    "build": "yarn build:esm && yarn build:cjs",
    "postbuild": "rm -rf ./dist/*.tsbuildinfo",
    "lint": "yarn eslint src",
    "lint:fix": "yarn lint --fix src",
    "test": "yarn jest",
    "test:coverage": "yarn jest --coverage",
    "format": "prettier --write \"./src/**/*.{ts,tsx}\"",
    "clear": "rm -rf coverage && rm -rf dist && rm -f *.tsbuildinfo",
    "clear:deps": "rm -rf ./node_modules && rm -rf ./dist"
  },
  "gitHead": "ed2cec449e634a16dc3a80b4a8cd2d1d28c9f9c4"
}
